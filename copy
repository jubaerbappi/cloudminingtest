	<script type="text/javascript">
			
			$(window).ready(function() {
			
            $("#Login").submit(function(e){
                e.preventDefault();

                var email = $("#login-email").val();
				var password = $("#login-pass").val();

                $.ajax({
                    url: '/augh/login.php',
                    type: "POST",
                    data: "email=" + email + "&password=" + password + "&g-recaptcha-response=" + grecaptcha.getResponse(0),
                    success: function(data) {
                        if (!data.success) {
							$("#login-error").text(data.message);	
							console.log(data.success);
                            grecaptcha.reset(0);
							if (data.code == 2)
							{
								grecaptcha.reset(0);	
							}
						
						} 
						else if (data.success)
							{
								console.log(data.success);
								document.location = "/dashboard";
							}
						
                    }
                })
            });
			
			});
				
	</script>
  <script type="text/javascript">
  
			$(window).ready(function() {
			
            $("#Registration").submit(function(e){
                e.preventDefault();

                var email 		= $("#reg-email").val();
				var login 		= $("#reg-login").val();
				var password 	= $("#reg-pass").val();
				var password_2 	= $("#reg-pass-2").val();
				

                $.ajax({
                    url: '/augh/signup.php',
                    type: "POST",
                    data: "email=" + email + "&login=" + login + "&password=" + password + "&password_2=" + password_2 + "&g-recaptcha-response=" + grecaptcha.getResponse(1),
                    success: function(data) {
                        if (!data.success) {
							$("#reg-error").text(data.message);	
							console.log(data.success);
                            grecaptcha.reset(1);	
							if (data.code == 2)
							{
								grecaptcha.reset(1);	
							}
						
						} 
						else if (data.success)
							{
								console.log(data.success);
								document.location = "/dashboard";
							}
						
                    }
                })
            });
			
			});
			
			</script>
			
			<script type="text/javascript">
			
			$(window).ready(function() {
			
            $("#Recovery").submit(function(e){
                e.preventDefault();

                var email = $("#rec-email").val();
				
                $.ajax({
                    url: '/augh/recover.php',
                    type: "POST",
                    data: "email=" + email  + "&g-recaptcha-response=" + grecaptcha.getResponse(2),
                    success: function(data) {
                        if (!data.success) {
							$("#rec-error").text(data.message);	
							
                            grecaptcha.reset(2);	
							if (data.code == 2)
							{
								grecaptcha.reset(2);	
							}
						
						} 
						else if (data.success)
							{
								$("#rec-error").text(data.message);	
								console.log(data.success);
								//document.location = "/";
							}
						
                    }
                })
            });
			
			$(".default__btn_forget").click(function () {
				$(".modal__sign-in").removeClass("visible");
				$(".modal__recover").addClass("visible");
			});
			
			
			});
			
			
			
			</script>
  
  <script>
	
		
	$(window).ready(function() {
		

		function loadData() {
		  return new Promise((resolve, reject) => {
			setTimeout(resolve, 500);
		  })
		}

		loadData()
		  .then(() => {
			let preloaderEl = document.getElementById('overlay');
			$('#overlay').fadeOut("fast");
		  });
	
		
		
		
		url = window.location.pathname.substring(1);
		var body = document.body;
		//alert(url);
		
		switch(url) {
			
			case 'about':
				body.classList.add("about-bg");
			break;
			
			case 'news':
				body.classList.add("main-bg");
			break;
			
			case 'support':
				body.classList.add("support-bg");
			break;
			
			case 'partners':
				body.classList.add("partners-bg");
			break;
			
			case 'marketing':
				body.classList.add("main-bg");
			break;
			
			case 'statistics':
				body.classList.add("main-bg");
			break;
			
			case 'mining':
				body.classList.add("mining-bg");
			break;
			
			case 'account':
				body.classList.add("account-bg");
			break;
			
			case 'reward':
				body.classList.add("account-bg");
			break;
			
			case 'purchase':
				body.classList.add("account-bg");
			break;
			
			case 'history':
				body.classList.add("account-bg");
			break;
			
			default:
				body.classList.add("section-bg");
			break;
			
		}      
		
		$(".profile-name.title").text(url)
		
	});
	
	
	
		
		
	</script>
	
