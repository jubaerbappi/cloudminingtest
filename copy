<script>
var $root = $('html, body');

$('a[href^="#"]').click(function() {
    $root.animate({
        scrollTop: $($.attr(this, 'href')).offset().top
    }, 500);

    return false;
});

function copy_text_fun() {
    //getting text from P tag
    var copyText = document.getElementById("referral__link");
    // creating textarea of html
    var input = document.createElement("textarea");
    //adding p tag text to textarea 
    input.value = copyText.textContent;
    document.body.appendChild(input);
    input.select();
    document.execCommand("Copy");
    // removing textarea after copy
    input.remove();

}
$('.crypto-items__slider').slick({
    infinite: true,
    centerMode: true,
    slidesToShow: 5,
    slidesToScroll: 1,
    variableWidth: true,
    autoplay: true,
    arrows: false,
    dots: false,
});
/// Main range slider
var $rangeslider = $('#js-amount-range');
var $amount = $('#js-amount-input');

$rangeslider
    .rangeslider({
        polyfill: false
    })
    .on('input', function() {
        let temp = this.value;
        $amount[0].value = temp;
        calculator();
    });

$amount.on('input', function() {
    let temp = parseFloat($(this).val().replace(',', '.'));
    
    if(!temp){
        temp = 0;
    }

    $rangeslider.val(temp).change();
});
$(".user__sin-ing-button").click(function() {
    $(".signiIn__modal").toggleClass("visible");
});
$(".close__modal").click(function() {
    $(".modal__overflow ").removeClass("visible");
});
$(".modal__create-button,.sign__up-button").click(function() {
    $(".signiIn__modal ").removeClass("visible");
    $(".signiUp__modal").toggleClass("visible");
});
$(".modal__forgot-link").click(function() {
    $(".signiIn__modal ").removeClass("visible");
    $(".modal__forgot").toggleClass("visible");
});
$(".crypto__item-input.drop__menu").click(function() {
    $(".crypto__item-input.drop__menu").removeClass("active");
    $(this).addClass("active");
});

$(".mining_calc .crypto__drop-down-list ul li").click(function() {
    
    $(".mining_calc").removeClass("active");
    $(".mining_calc i").first().remove();
    $(this).find('i').clone().prependTo(".mining_calc");
    $(".mining_calc input").attr('placeholder',$(this).text().trim());
    $(".mining_calc input").val('');
    $(".mining_calc li").show();

    var short_name = $(this).attr('short-name');
    $('#mining_curr').text(short_name);
    
    calculator();

});

$(".mining_calc input").keyup(function () {  
    
    let val = $(".mining_calc input").val();
    $( ".mining_calc .crypto__select-item" ).each(function() {
        let li  = $(this).text().trim().toLowerCase();
        if(li.indexOf(val) >= 0){
            $(this).parent().show();
        }
        else{
            $(this).parent().hide();
        }
    });

});

$(".invest_calc .crypto__drop-down-list ul li").click(function() {
    
    $(".invest_calc").removeClass("active");
    $(".invest_calc i").first().remove();
    $(this).find('i').clone().prependTo(".invest_calc");
    $(".invest_calc input").attr('placeholder',$(this).text().trim());
    $(".invest_calc input").val('');
    $(".invest_calc li").show();

    var short_name = $(this).attr('short-name');
    $('#invest_curr').text(short_name);

    calculator();

});

$(".invest_calc input").keyup(function () {  
    
    let val = $(".invest_calc input").val();
    $( ".invest_calc .crypto__select-item" ).each(function() {
        let li  = $(this).text().trim().toLowerCase();
        if(li.indexOf(val) >= 0){
            $(this).parent().show();
        }
        else{
            $(this).parent().hide();
        }
    });

});

$(".faq__item").click(function() {
    $(this).toggleClass("active");
});
jQuery(function($) {
    $(document).mouseup(function(e) {
        var div = $(".crypto__item-input.drop__menu.active");
        if (!div.is(e.target)
            &&
            div.has(e.target).length === 0) { 
            $('.crypto__item-input.drop__menu.active').removeClass('active');
        }
    });
});
</script>
